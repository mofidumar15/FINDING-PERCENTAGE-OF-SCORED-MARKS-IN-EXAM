#include <iostream>
using namespace std;

int main() {
    float mathematics, physics, bcem, bce, lls;
    
    cout << "Enter Mathematics marks: ";
    cin >> mathematics;
    
    cout << "Enter Physics marks: ";
    cin >> physics;
    
    cout << "Enter BCem marks: ";
    cin >> bcem;
    
    cout << "Enter Bce marks: ";
    cin >> bce;
    
    cout << "Enter LLS marks: ";
    cin >> lls;
    
    float total = mathematics + physics + bcem + bce + lls;
    float percentage = total / 5.0;
    float t = percentage*5;
    
    cout << "Percentage of your overall marks of midsem is = " << percentage <<"% "<< endl;
  cout <<"   and ,, YOUR TOTAL SCORE IS "<< t<< endl;

    return 0;
}
